{
  "name": "hysteria2-vpn-client",
  "version": "1.0.0",
  "description": "Cross-platform VPN client supporting Hysteria2 protocol",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:ios": "cd ios && xcodebuild -project hysteria2_vpn_client.xcodeproj -scheme hysteria2_vpn_client -configuration Release archive -archivePath hysteria2_vpn_client.xcarchive",
    "build:ios:simulator": "cd ios && xcodebuild -project hysteria2_vpn_client.xcodeproj -scheme hysteria2_vpn_client -configuration Release -sdk iphonesimulator -destination 'platform=iOS Simulator,name=iPhone 16' build",
    "build:macos": "cd macos && xcodebuild -project Hysteria2VpnClient.xcodeproj -scheme Hysteria2VpnClient -configuration Release archive -archivePath Hysteria2VpnClient.xcarchive",
    "build:windows": "cd windows && msbuild Hysteria2VpnClient.sln /p:Configuration=Release /p:Platform=x64",
    "build:linux": "cd linux && cmake . && make",
    "build:web": "DISABLE_ESLINT_PLUGIN=true npx react-scripts build",
    "build:docker": "docker build -t hysteria2-vpn-client ."
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.17",
    "react-native-mmkv": "^2.10.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-splash-screen": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/jest": "^29.5.0",
    "@testing-library/react-native": "^12.4.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "@react-native-community/cli": "^20.0.1",
    "@react-native-community/cli-doctor": "^20.0.1",
    "@react-native-community/cli-hermes": "^13.6.9"
  },
  "keywords": [
    "vpn",
    "hysteria2",
    "react-native",
    "cross-platform",
    "mobile",
    "desktop"
  ],
  "author": "Your Name",
  "license": "MIT"
}
